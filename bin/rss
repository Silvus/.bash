#!/usr/bin/env bash

rss_url="${DOCUMENTATION_HOME}/.data/rss/urls"
rss_cache="/data/tools/rss/cache.db"

# If run under Tmux, open in new window
if [[ -n "$TMUX" ]]; then
	tmux new-window -n "rss" "newsbeuter -q -u \"${rss_url}\" -c \"${rss_cache}\""
else
	newsbeuter -q -u "${rss_url}" -c "${rss_cache}"
fi

exit 0
