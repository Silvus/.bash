[
	// Save
	{
		"key": "ctrl+s",
		"command": "workbench.action.files.saveAll"
	},
	{
		"key": "ctrl+s",
		"command": "-workbench.action.files.save"
	},
	// File switch
	{
		"key": "ctrl+e",
		"command": "workbench.action.quickOpenPreviousEditor"
	},
	{
		"key": "ctrl+e",
		"command": "workbench.action.quickOpenNavigateNextInFilePicker",
		"when": "inFilesPicker && inQuickOpen"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenPreviousEditor"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenNavigateNextInFilePicker",
		"when": "inFilesPicker && inQuickOpen"
	},
	{
		"key": "ctrl+p",
		"command": "-workbench.action.quickOpen"
	},
	{
		"key": "ctrl+r",
		"command": "workbench.action.gotoSymbol"
	},
	{
		"key": "ctrl+shift+r",
		"command": "workbench.action.showAllSymbols"
	},
	// Workspace switch
	{
		"key": "ctrl+shift+w",
		"command": "workbench.action.openRecent"
	},
	// Splits
	{
		"key": "ctrl+a v",
		"command": "workbench.action.splitEditor"
	},
	{
		"key": "ctrl+a h",
		"command": "workbench.action.splitEditorOrthogonal"
	},
	{
		"key": "ctrl+h",
		"command": "workbench.action.splitEditor"
	},
	{
		"key": "ctrl+h",
		"command": "-editor.action.startFindReplaceAction"
	},
	// Splits navigation
	{
		"key": "ctrl+enter",
		"command": "workbench.action.navigateEditorGroups"
	},
	{
		"key": "ctrl+enter",
		"command": "-editor.action.insertLineAfter",
	},
	{
		"key": "ctrl+a o",
		"command": "workbench.action.navigateEditorGroups"
	},
	{
		"key": "ctrl+a right",
		"command": "workbench.action.navigateRight"
	},
	{
		"key": "alt+right",
		"command": "workbench.action.navigateRight"
	},
	{
		"key": "ctrl+a left",
		"command": "workbench.action.navigateLeft"
	},
	{
		"key": "alt+left",
		"command": "workbench.action.navigateLeft"
	},
	{
		"key": "ctrl+a up",
		"command": "workbench.action.navigateUp"
	},
	{
		"key": "alt+up",
		"command": "workbench.action.navigateUp"
	},
	{
		"key": "alt+up",
		"command": "-editor.action.moveLinesUpAction",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+a down",
		"command": "workbench.action.navigateDown"
	},
	{
		"key": "alt+down",
		"command": "workbench.action.navigateDown"
	},
	{
		"key": "alt+down",
		"command": "-editor.action.moveLinesDownAction",
		"when": "editorTextFocus && !editorReadonly"
	},
	// Splits move clients
	{
		"key": "ctrl+a shift+right",
		"command": "workbench.action.moveEditorToRightGroup"
	},
	{
		"key": "ctrl+a ctrl+right",
		"command": "workbench.action.moveEditorToRightGroup"
	},
	{
		"key": "shift+alt+right",
		"command": "workbench.action.moveEditorToRightGroup"
	},
	{
		"key": "shift+alt+right",
		"command": "-editor.action.smartSelect.expand",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+a shift+left",
		"command": "workbench.action.moveEditorToLeftGroup"
	},
	{
		"key": "ctrl+a ctrl+left",
		"command": "workbench.action.moveEditorToLeftGroup"
	},
	{
		"key": "shift+alt+left",
		"command": "workbench.action.moveEditorToLeftGroup"
	},
	{
		"key": "shift+alt+left",
		"command": "-editor.action.smartSelect.shrink",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+a shift+down",
		"command": "workbench.action.moveEditorToDownGroup"
	},
	{
		"key": "ctrl+a ctrl+down",
		"command": "workbench.action.moveEditorToDownGroup"
	},
	{
		"key": "shift+alt+down",
		"command": "workbench.action.moveEditorToBelowGroup"
	},
	{
		"key": "shift+alt+down",
		"command": "-editor.action.insertCursorBelow",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+a shift+up",
		"command": "workbench.action.moveEditorToUpGroup"
	},
	{
		"key": "ctrl+a ctrl+up",
		"command": "workbench.action.moveEditorToUpGroup"
	},
	{
		"key": "shift+alt+up",
		"command": "workbench.action.moveEditorToAboveGroup"
	},
	{
		"key": "shift+alt+up",
		"command": "-editor.action.insertCursorAbove",
		"when": "editorTextFocus"
	},
	// New tab
	{
		"key": "ctrl+t",
		"command": "workbench.action.files.newUntitledFile"
	},
	{
		"key": "ctrl+t",
		"command": "-workbench.action.showAllSymbols"
	},
	// Multi cursor
	{
		"key": "ctrl+l",
		"command": "editor.action.addSelectionToNextFindMatch",
		"when": "editorFocus"
	},
	{
		"key": "ctrl+l",
		"command": "-expandLineSelection",
		"when": "textInputFocus"
	},
	{
		"key": "ctrl+k",
		"command": "editor.action.selectHighlights",
		"when": "editorFocus"
	},
	{
		"key": "ctrl+shift+l",
		"command": "editor.action.insertCursorAtEndOfEachLineSelected",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+l",
		"command": "-editor.action.selectHighlights",
		"when": "editorFocus"
	},
	// Delete line
	{
		"key": "ctrl+d",
		"command": "editor.action.deleteLines",
		"when": "textInputFocus && !editorReadonly"
	},
	{
		"key": "ctrl+d",
		"command": "-editor.action.addSelectionToNextFindMatch",
		"when": "editorFocus"
	},
	// Folds
	{
		"key": "ctrl+shift+space",
		"command": "editor.toggleFold",
		"when": "editorTextFocus && foldingEnabled"
	},
	// Markdown extension is changing ctrl+b to bold
	{
		"key": "ctrl+b",
		"command": "-markdown.extension.editing.toggleBold",
		"when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
	},
	// Markdown preview
	{
		"key": "ctrl+shift+v",
		"command": "-markdown.showPreview",
		"when": "editorLangId == 'markdown'"
	},
	{
		"key": "ctrl+shift+v",
		"command": "markdown.showPreviewToSide",
		"when": "editorLangId == 'markdown'"
	},
]