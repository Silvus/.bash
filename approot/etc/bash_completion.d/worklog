_worklog()
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    opts="add edit hello stretch report --help --now --current-activity --no-current-activity"

    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
}
complete -F _worklog worklog
